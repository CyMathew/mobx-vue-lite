{
  "name": "mobx-vue-lite",
  "version": "0.4.2",
  "license": "MIT",
  "sideEffects": true,
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    },
    "./nuxt": "./nuxt/module.mjs"
  },
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "files": [
    "dist",
    "nuxt"
  ],
  "scripts": {
    "build-fast": "tsup src/index.ts --format esm,cjs",
    "build": "pnpm run build-fast --dts-resolve",
    "test": "vitest run",
    "release": "bumpp --commit --push --tag && pnpm publish",
    "prepublishOnly": "pnpm run build",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix"
  },
  "peerDependencies": {
    "mobx": "^6.1.0",
    "vue": "^3.2.0"
  },
  "dependencies": {
    "@vueuse/core": "10.7.2"
  },
  "devDependencies": {
    "@antfu/eslint-config": "2.6.4",
    "@types/node": "18.16.18",
    "@vue/test-utils": "2.4.4",
    "bumpp": "9.1.1",
    "eslint": "8.56.0",
    "happy-dom": "9.20.3",
    "mobx": "^6.13.1",
    "tsup": "7.0.0",
    "typescript": "5.1.3",
    "vitest": "1.2.2",
    "vue": "^3.4.37"
  }
}
