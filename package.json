{
  "name": "mobx-vue-lite",
  "version": "0.3.1",
  "license": "MIT",
  "sideEffects": true,
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.js",
      "import": "./dist/index.mjs"
    },
    "./nuxt": "./nuxt/module.mjs"
  },
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "files": [
    "dist",
    "nuxt"
  ],
  "scripts": {
    "build-fast": "tsup src/index.ts --format esm,cjs",
    "build": "pnpm run build-fast -- --dts-resolve",
    "test": "vitest run",
    "release": "bumpp --commit --push --tag && pnpm publish",
    "prepublishOnly": "pnpm run build",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix"
  },
  "peerDependencies": {
    "mobx": "^6.1.0",
    "vue": "^3.2.0"
  },
  "devDependencies": {
    "@antfu/eslint-config": "0.39.5",
    "@types/node": "18.16.18",
    "@vue/test-utils": "2.3.2",
    "@vueuse/core": "10.2.0",
    "bumpp": "9.1.1",
    "eslint": "8.42.0",
    "happy-dom": "9.20.3",
    "mobx": "^6.9.0",
    "tsup": "7.0.0",
    "typescript": "5.1.3",
    "vitest": "0.32.1",
    "vue": "^3.3.4"
  },
  "eslintConfig": {
    "extends": "@antfu"
  }
}
